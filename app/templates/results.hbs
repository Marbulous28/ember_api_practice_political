{{#if model}}
  <ul>
    {{#each model as |legislator|}}
      <li> {{legislator.first_name}} {{legislator.last_name}}: {{legislator.chamber}} - {{legislator.party}}<br>Contact: <a href={{legislator.contact_form}}>{{legislator.contact_form}}</a></li>
    {{/each}}
  </ul>
{{else}}
  <h2>That area code doesn't exist</h2>
  {{#link-to "index"}}go back{{/link-to}}
{{/if}}
